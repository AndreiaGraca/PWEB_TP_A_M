<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Laboratório A&M</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>

        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <div>
                    <img src="images\Logo1.PNG" alt="Logo" width="50" height="50">
                </div>
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Laboratório A&M</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>


                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1" id="navbar">

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index" onmouseover="myFunction(this)" onmouseout="myFunctionAA(this)">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="CentroTeste" asp-action="Index" onmouseover="myFunction(this)" onmouseout="myFunctionAA(this)">CentroTeste</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Analise" asp-action="Index" onmouseover="myFunction(this)" onmouseout="myFunctionAA(this)">Analises</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Localizacao" asp-action="Index" onmouseover="myFunction(this)" onmouseout="myFunctionAA(this)">Localizações</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Analise" asp-action="Index" onmouseover="myFunction(this)" onmouseout="myFunctionAA(this)">Contactos</a>
                        </li>

                        <script>
                            function myFunction(x) {
                                x.style.background = "SteelBlue";

                            }

                            function myFunctionAA(x) {
                                x.style.background = "white";
                            }
                        </script>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
<div class="container">

    <main role="main" class="pb-3">
        @RenderBody()

    </main>
</div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - Laboratorio A&M - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
